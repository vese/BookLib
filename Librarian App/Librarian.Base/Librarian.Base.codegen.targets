<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
<!--=============================================================================================-->

<Target
   Name="_GenerateFile_Librarian_Base_Info_CS_">
   <Message
      Text="* Generating file: &quot;$(_OutputFile_Librarian_Base_Info_CS_)&quot; ..."
      Importance="high" />
   <WriteLinesToFile
      File="$(_OutputFile_Librarian_Base_Info_CS_)"
      Lines="@(_Librarian_Base_Info_CS_->'%(Text)')"
      Encoding="UTF-8"
      Overwrite="True" />
</Target>
