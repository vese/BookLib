</Target>
-->

<Target
   Name="_CopyFilesToMetaOutputFolder_"
   Condition=" '$(_MetaOutputFolder_)' != '' AND
               '@(_CopyFilesToMetaOutputFolder_Files_)' != '' ">
   <Message
      Text="* Copying files to meta output folder ..."
      Importance="high" />
   <Copy
      SourceFiles='@(_CopyFilesToMetaOutputFolder_Files_)'
      DestinationFolder='$(_MetaOutputFolder_)' />
</Target>

<Target
   Name="_GenerateCode_"
   DependsOnTargets=" _GenerateFile_Librarian_Base_Info_CS_;
                      _CopyFilesToMetaOutputFolder_ " />

<!--=============================================================================================-->
</Project>
